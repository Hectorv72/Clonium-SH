<style>

    table{
        background-color:black;
        border: 4px solid black;
    }
    td{
        text-align: center;
        width: 50px;
        height: 50px;
        background-color: rgb(151, 229, 229);
    }
    .ficha {
        width: 40px;
        height: 40px;
        margin: 5%;
        border-radius: 50px;
        border-color: black;
        border-width: 2px;
        border-style: solid;
        text-align: center;
        font-family: "Fira Code";
    }


    .dot::before{
        color: white;
        position: relative;
        font-size: 150%
        /* margin-top: 80%; */
    }

    .dot-1::before{
        content: '∙';
        top: 15%;
    }
    .dot-2::before{
        content: '∙∙';
        top: 15%;
    }
    .dot-3::before{
        content: '∴';
        /* top: 5%; */
        font-size: 200%
    }
    .dot-4::before{
        content: '::';
        top: 10%;
    }
    .dot-5::before{
        content: '5';
        top: 10%;
    }
</style>
<body>
    <div class="center" >
        <div id="root"></div>

        <table id="tabla" ></table><br>
        <div id="puntos"></div>
        <!-- <h1 id="mensaje"></h1> -->
    </div>

</body>
